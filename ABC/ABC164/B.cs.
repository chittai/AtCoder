using System;
using System.Linq;
using System.Collections.Generic;

namespace ABC164
{
    class B
    {
        static void Main(string[] args)
        {
            var ABCD = Console.ReadLine().Split().Select(double.Parse).ToArray();
            var A = ABCD[0];
            var B = ABCD[1];
            var C = ABCD[2];
            var D = ABCD[3];

            var tcount = 0;
            var acount = 0;
            for (int i = 0; 0 < A; i++)
            {
                tcount++;
                A -= D;
            }

            for (int i = 0; 0 < C; i++)
            {
                acount++;
                C -= B;
            }

            if (acount <= tcount) Console.WriteLine("Yes");
            else Console.WriteLine("No");


        }
    }
}
